<table cellpadding="0" cellspacing="0">
  <thead>
    <tr>
      <!-- render :partial => 'list_column_headings' was here -->
    </tr>
  </thead>

  <%= render :partial => 'shared/as_messages' %>

  <tbody class="records" id="<%= active_scaffold_tbody_id %>">
    <%= render :partial => 'list_record', 
      :collection => @page.items, 
      :locals => { :hidden => false, :dont_show_calculations => true } %>
    <% if active_scaffold_config.list.columns.any? {|c| c.calculation?} -%>
      <%= render :partial => 'list_calculations' %>
    <% end -%>
  </tbody>
</table>
